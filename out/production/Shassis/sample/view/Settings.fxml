         <items>
            
         </items>
      </ChoiceBox>
      <CheckBox fx:id="ZIP" layoutX="20.0" layoutY="99.0" mnemonicParsing="false" text="ЗИП" AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="100.0" />
      <CheckBox fx:id="recheck" layoutX="20.0" layoutY="128.0" mnemonicParsing="false" onAction="#setRecheck" text="Перепроверка" AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="125.0" />
