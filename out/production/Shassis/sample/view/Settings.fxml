<AnchorPane maxHeight="500.0" maxWidth="280.0" minHeight="444.0" minWidth="280.0" prefHeight="444.0" prefWidth="280.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.view.SettingsController">
