      <CheckBox fx:id="noFUN" layoutX="20.0" layoutY="150.0" mnemonicParsing="false" onAction="#setNoFUN" text="Шасси без FUN" AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="150.0" />
      <TextField fx:id="backPlanePN" layoutX="180.0" layoutY="300.0" prefHeight="25.0" prefWidth="80.0" AnchorPane.bottomAnchor="175.0" AnchorPane.rightAnchor="20.0" />
      <TextField fx:id="FUNPN" layoutX="180.0" layoutY="335.0" prefHeight="25.0" prefWidth="80.0" AnchorPane.bottomAnchor="140.0" AnchorPane.rightAnchor="20.0" />
      <Label layoutX="20.0" layoutY="303.0" text="Номер платы BackPlane" AnchorPane.bottomAnchor="180.0" AnchorPane.leftAnchor="20.0" />
      <Label layoutX="20.0" layoutY="338.0" text="Номер платы FUN" AnchorPane.bottomAnchor="145.0" AnchorPane.leftAnchor="20.0" />
      <Button layoutX="214.0" layoutY="461.0" mnemonicParsing="false" onAction="#checkValid" text="Готово" AnchorPane.bottomAnchor="15.0" AnchorPane.rightAnchor="15.0" />
      <Button layoutX="10.0" layoutY="460.0" mnemonicParsing="false" onAction="#setLastSettings" text="Выставить последние настройки" AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="10.0" />
      <ChoiceBox fx:id="fanModel" layoutX="20.0" layoutY="410.0" prefWidth="150.0" AnchorPane.bottomAnchor="65.0" AnchorPane.leftAnchor="20.0" />
      <Label layoutX="25.0" layoutY="380.0" text="Модель вентиляторов" AnchorPane.bottomAnchor="103.0" AnchorPane.leftAnchor="25.0" />
   </children>
</AnchorPane>
